{
  "name": "hsnbd/auditlog",
  "description": "Elasticsearch auditlog for laravel application.",
  "keywords": [
    "log",
    "audit",
    "audit-log",
    "auditlog",
    "auditlogger",
    "laravel",
    "elasticsearch",
    "elk",
    "logs"
  ],
  "homepage": "https://github.com/hsnbd/auditlog",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Baker Hasan",
      "email": "hasanbd666@gmail.com"
    }
  ],
  "require": {
    "elasticsearch/elasticsearch": "^7.10",
    "php": ">=7.4",
    "laravel/framework": ">=5.7 <=8.9"
  },
  "require-dev": {
    "ext-json": "*",
    "orchestra/testbench": "~3.6.7 || ~3.7.8 || ~3.8.6 || ^4.8 || ^5.2 || ^6.0",
    "phpunit/phpunit": "^6.0 || ^7.0 || ^8.0 || ^9.0"
  },
  "autoload": {
    "psr-4": {
      "Hsnbd\\AuditLogger\\": "/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hsnbd\\AuditLogger\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hsnbd\\AuditLogger\\AuditLoggerServiceProvider"
      ],
      "aliases": {
        "AuditLog": "Hsnbd\\AuditLogger\\Facades\\AuditLog"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "vendor\\bin\\phpunit"
  }
}
